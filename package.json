  "license": "MIT",
  "dependencies": {
    "@types/cors": "^2.8.6",
    "axios": "^0.19.0",
    "concurrently": "^5.2.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^14.6.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/graphql": "^14.5.0",
    "@types/node": "^16.4.10",
    "nodemon": "^2.0.4",
    "typescript": "^3.9.3"
  },
  "volta": {
    "node": "12.13.0"
  }
}
