{
  "name": "livescore",
  "version": "1.0.0",
  "description": "App that shows livescores in real-time",
  "main": "server.js",
  "scripts": {
    "start": "node dist/server.js",
    "server": "nodemon dist/server.js",
    "client": "yarn --cwd ./client run start",
    "dev": "concurrently  -k \"yarn tsc -w\" \"yarn run server\" \"yarn run client\""
  },
